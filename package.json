{
  "name": "medspasync-marketing",
  "version": "1.0.0",
  "private": true,
  "description": "MedSpaSync Pro - AI-powered reconciliation for medical spas",
  "type": "module",
  "keywords": [
    "medical-spa",
    "healthcare",
    "reconciliation",
    "ai",
    "react",
    "vite"
  ],
  "author": "Jacob Hagood",
  "license": "MIT",
  "homepage": "https://medspasyncpro.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/MythosMediaCo/medspasync-marketing.git"
  },
  "bugs": {
    "url": "https://github.com/medspasyncpro/marketing/issues",
    "email": "support@medspasyncpro.com"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "clean": "rimraf dist node_modules/.vite",
    "sitemap": "node scripts/generate-sitemap.js",
    "prebuild": "npm run clean",
    "postbuild": "npm run sitemap",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:github": "npm run build && git add dist && git commit -m 'Deploy to GitHub Pages' && git push",
    "start": "vite preview --port 3000",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@azure/identity": "^4.0.1",
    "@azure/keyvault-secrets": "^4.7.3",
    "@prisma/client": "^6.11.1",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.10.0",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "exceljs": "^4.4.0",
    "fuzzball": "^2.2.2",
    "mongoose": "^8.16.1",
    "nodemailer": "^7.0.4",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.8.1",
    "recharts": "^3.0.2",
    "supertest": "^7.1.1",
    "xlsx": "^0.18.5",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "@lhci/cli": "^0.15.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "^24.0.0",
    "lighthouse": "^12.6.1",
    "papaparse": "^5.5.3",
    "postcss": "^8.4.27",
    "postcss-custom-properties": "^14.0.6",
    "postcss-focus-visible": "^10.0.1",
    "postcss-gap-properties": "^6.0.0",
    "postcss-import": "^16.1.1",
    "postcss-logical": "^8.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-reporter": "^7.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "terser": "^5.43.1",
    "typescript": "^5.4.3",
    "vite": "^4.4.5",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^0.34.6",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "port": 3000,
    "apiUrl": {
      "development": "http://localhost:3001",
      "staging": "https://api-staging.medspasyncpro.com",
      "production": "https://api.medspasyncpro.com"
    }
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "resolutions": {
    "semver": "^7.5.4"
  },
  "volta": {
    "node": "18.19.1",
    "npm": "10.2.4"
  }
}
