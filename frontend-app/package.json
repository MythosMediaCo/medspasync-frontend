{
  "name": "medspasync-marketing",
  "version": "1.0.0",
  "private": true,
  "description": "MedSpaSync Pro - AI-powered reconciliation for medical spas",
  "type": "module",
  "keywords": [
    "medical-spa",
    "healthcare",
    "reconciliation",
    "ai",
    "react",
    "vite"
  ],
  "author": "Jacob Hagood",
  "license": "MIT",
  "homepage": "https://medspasyncpro.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/MythosMediaCo/medspasync-marketing.git"
  },
  "bugs": {
    "url": "https://github.com/medspasyncpro/marketing/issues",
    "email": "support@medspasyncpro.com"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --reporter=verbose",
    "test:coverage:ci": "vitest --coverage --reporter=verbose --coverage.threshold.lines=80 --coverage.threshold.functions=80 --coverage.threshold.branches=80 --coverage.threshold.statements=80",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "clean": "rimraf dist node_modules/.vite",
    "sitemap": "node scripts/generate-sitemap.js",
    "prebuild": "npm run clean",
    "postbuild": "npm run sitemap",
    "deploy": "npm run build && npm run deploy:vercel",
    "deploy:vercel": "vercel --prod --yes",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:github": "npm run build && git add dist && git commit -m 'Deploy to GitHub Pages' && git push",
    "start": "vite preview --port 3000",
    "analyze": "vite-bundle-analyzer"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@hookform/resolvers": "^5.1.1",
    "@playwright/test": "^1.54.1",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.3",
    "lucide-react": "^0.263.1",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.8.1",
    "recharts": "^3.0.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.0.0",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.27",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.3.3",
    "terser": "^5.43.1",
    "typescript": "^5.4.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "port": 3000,
    "apiUrl": {
      "development": "http://localhost:3001",
      "staging": "https://api-staging.medspasyncpro.com",
      "production": "https://api.medspasyncpro.com"
    }
  },
  "volta": {
    "node": "18.19.1",
    "npm": "10.2.4"
  }
}
